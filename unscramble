f=open("/share/data/day5/puzzle.txt")
lines=f.readlines()
o=15
p=0
for i in lines:
        print lines[p:o]
        o=o+15
        p=p+15


U=""
V=""
W=""
B=""
for i in lines:
	if "SSS" in i:
		U=U+i
	if "PPRPPRP" in i:
		W=W+i
	if "KV" in i:
		B=B+i	
	else:
		V=V+i
print "these have SSS"
print U
print "These have KV"
print B
print "these have PPRPPRP"
print W
print "garbage leftovers"
print V

lines="".join(lines)


print lines
