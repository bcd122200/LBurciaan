f=open("/share/data/day5/puzzle.txt")
U=""
V=""
j=raw_input("Enter sequence: ")
X=""
W=""
lines=f.readlines()
for i in lines:
	j=i.split()
	#print j
for i in j:
	if "SSS" in i:
		U=U+i
	if "PPRPPRP" in i:
		W=W+i
	if j in i:
		X=X+i
	else:
		V=V+i
print "these have SSS"
print U
print "these have PPRPPRP"
print W
print "what u want bb"
print X
print "garbage leftovers"
print V

